{
  "name": "svelte-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/server.ts",
  "scripts": {
    "build": "npm-run-all clean compile",
    "build-dev": "npm-run-all clean compile-dev",
    "clean": "rimraf ./dist",
    "compile": "npm-run-all generate-client-components webpack-compile",
    "compile-dev": "npm-run-all generate-client-components webpack-compile-dev",
    "dev": "npm-run-all build-dev --parallel watch-pages watch-files watch-server livereload",
    "develop-mode-manual": "npm-run-all build-dev start",
    "generate-client-components": "node npm-scripts/generate-client-component-files.js",
    "livereload": "livereload dist",
    "start": "node dist/server.js",
    "watch-files": "cross-env NODE_ENV=development webpack watch",
    "watch-pages": "chokidar \"src/pages/**\" -c \"npm run generate-client-components\" --initial --silent",
    "watch-server": "nodemon dist/server.js",
    "webpack-compile": "cross-env NODE_ENV=production webpack",
    "webpack-compile-dev": "cross-env NODE_ENV=development webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.0",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.8",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "chokidar": "^3.5.2",
    "fastify": "^3.22.0",
    "fastify-socket.io": "^2.0.1",
    "fastify-static": "^4.4.2",
    "long": "^4.0.0",
    "pino-pretty": "^7.1.0",
    "socket.io": "^4.3.0",
    "socket.io-client": "^4.3.1",
    "svelte": "^3.43.2"
  }
}
